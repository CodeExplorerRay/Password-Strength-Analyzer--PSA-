{
  "version": 2,
  "builds": [
    {
      "src": "analyze.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "templates/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "analyze.py"
    },
    {
      "src": "/(.*)",
      "dest": "/templates/$1"
    }
  ]
}
