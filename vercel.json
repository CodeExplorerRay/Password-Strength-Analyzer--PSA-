{
  "version": 2,
  "builds": [
    {
      "src": "analyze.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/content/(.*)",
      "dest": "/content/$1"
    },
    {
      "src": "/(.*)",
      "dest": "analyze.py"
    }
  ]
}
